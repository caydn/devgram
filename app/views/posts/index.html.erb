<p id="notice"><%= notice %></p>

<div id="starter" class="container">
  <% @posts.each do |post| %>
    <div class="posts-wrapper row">
        <div class="post">
          <div class="post-head">
            <div class="name">Devgirl CJ</div>
          </div>
          <div class="image center-block">
            <%= link_to (image_tag post.image.full_size.url, class:'img-responsive'), post_path(post) %>
          </div>
          <!-- <p class="caption">
            <%= post.caption %>
          </p> -->
        </div>


    </div>
  <% end %>
</div>
